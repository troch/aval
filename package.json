{
  "name": "izit",
  "version": "0.1.0",
  "description": "A minimalist property validator",
  "main": "dist/commonjs/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint modules",
    "test": "node ./node_modules/mocha/bin/_mocha",
    "uglify": "./node_modules/.bin/uglifyjs2 dist/browser/izit.js --comments -m -o dist/browser/izit.min.js",
    "uglifyAmd": "./node_modules/.bin/uglifyjs2 dist/amd/izit.js --comments -m -o dist/amd/izit.min.js",
    "build": "node scripts/build.js && npm run uglify && npm run uglifyAmd",
    "test-cover": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/troch/izit.git"
  },
  "keywords": [
    "validator",
    "validation",
    "property",
    "types"
  ],
  "author": "Thomas Roch <thomas.c.roch@gmail.com> (http://blog.reactandbethankful.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/troch/izit/issues"
  },
  "homepage": "https://github.com/troch/izit",
  "devDependencies": {
    "async": "^1.4.0",
    "babel": "^5.8.3",
    "babel-eslint": "^3.1.26",
    "conventional-changelog": "^0.1.3",
    "coveralls": "^2.11.3",
    "eslint": "^0.24.1",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "should": "^7.0.2",
    "uglify-js2": "^2.1.11",
    "vinyl-fs": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
